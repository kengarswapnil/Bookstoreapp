{
  "version": 2,
  "name": "bookstoreapp",
  "builds": [
    {
      "src": "Frontend/vite-project/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd Frontend/vite-project && npm install && npm run build",
  "outputDirectory": "Frontend/vite-project/dist"
}
